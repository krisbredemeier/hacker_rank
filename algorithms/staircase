#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int count;
    scanf("%d", &count);
    for (int i=1; i < count+1; ++i){
        for(int j=0; j<count-i; ++j) 
        { 
            printf(" ");
        }
        for(int k=0; k<i; ++k) 
        {
            printf("#");
        }
        printf("\n");
    }    
    return 0;
}